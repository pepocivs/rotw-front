{
  "entrypoint": "index.html",
  "shell": "src/recipes-wild.html",
  "fragments": [
    "src/btw-form/btw-form.html",
    "src/btw-info/btw-info.html",
    "src/btw-initial/btw-initial.html",
    "src/btw-inventory/btw-inventory.html",
    "src/btw-items/btw-items.html",
    "src/btw-recipes/btw-recipes.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*",
    "bower.json",
    "manifest.json"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ],
  "lint": {
    "rules": ["polymer-1"]
  },
  "builds": [{
    "addServiceWorker": true,
    "swPrecacheConfig": "sw-precache-config.js",
    "bundle": true,
    "js": {"minify": false},
    "css": {"minify": true},
    "html": {"minify": true}
  }]
